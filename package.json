{
  "name": "ember-addon-starterkit",
  "version": "0.2.0",
  "description": "Starter kit for creating and publishing new Ember addons with all batteries included.",
  "keywords": [
    "ember-addon",
    "addon",
    "starterkit",
    "boilerplate",
    "ember"
  ],
  "license": "MIT",
  "author": "Rajasegar Chandran",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/rajasegar/ember-addon-starterkit",
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "npm version patch && git push && git push --tags && npm publish",
    "deploy-feat": "npm version minor && git push && git push --tags && npm publish"
  },
  "dependencies": {
    "ember-cli-babel": "^7.17.2",
    "ember-cli-htmlbars": "^4.2.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.16.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.0.0",
    "global-modules": "^2.0.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^1.0.0",
    "sort-package-json": "^1.44.0",
    "yarn-global-modules": "^1.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://rajasegar.github.io/ember-addon-starterkit/"
  }
}
